5.表设计
CREATE TABLE `menu` (
  `mid` tinyint(3) unsigned NOT NULL AUTO_INCREMENT,
  `menu_item_name` varchar(60) NOT NULL,
  `unit` varchar(30) DEFAULT NULL,
  PRIMARY KEY (`mid`)
) ENGINE=InnoDB AUTO_INCREMENT=15 DEFAULT CHARSET=utf8;

CREATE TABLE `inventory` (
  `in_id` tinyint(3) unsigned NOT NULL AUTO_INCREMENT,
  `inventory` int(3) unsigned zerofill NOT NULL,
  `mid` tinyint(3) unsigned NOT NULL,
  `item` varchar(30) NOT NULL,
  `u_date` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' ON UPDATE CURRENT_TIMESTAMP,
  PRIMARY KEY (`in_id`),
  KEY `inventory_mid_fk` (`mid`),
  CONSTRAINT `inventory_mid_fk` FOREIGN KEY (`mid`) REFERENCES `menu` (`mid`)
) ENGINE=InnoDB AUTO_INCREMENT=15 DEFAULT CHARSET=utf8;